public class App {
public static void main (String[] args) {
	String str1 = "summer";
	String str2 = "winter";
	System.out.println("str1= " + str1);
	System.out.println("str2= " + str2);
	
if(str1.equals(str2)){
      System.out.println("Строки идентичны");
    }else{
      System.out.println("Строки неидентичны");
    }
	
}
}	
